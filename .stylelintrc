{
  "extends": ["stylelint-config-standard-less"],
  "rules": {
    "selector-class-pattern": null,
    "declaration-empty-line-before": ["always", {
      "except": ["first-nested"],
      "ignore": ["after-comment", "after-declaration"]
    }]
  },
  "ignoreFiles": [
    "source/swiper/*.*"
  ]
}
