{
  "name": "blitz",
  "private": true,
  "description": "Агентство Blitz Estate",
  "devDependencies": {
    "@linthtml/linthtml": "0.9.5",
    "autoprefixer": "10.4.0",
    "browser-sync": "2.27.7",
    "browser-sync-client": "2.27.7",
    "del": "7.1.0",
    "gulp": "4.0.2",
    "gulp-htmlmin": "5.0.1",
    "gulp-imagemin": "9.0.0",
    "gulp-less": "5.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-rename": "2.0.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-svgstore": "9.0.0",
    "gulp-webp": "4.0.1",
    "postcss-csso": "6.0.1",
    "stylelint": "16.2.1",
    "stylelint-config-standard-less": "3.0.1",
    "webpack": "5.91.0",
    "webpack-stream": "7.0.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/less/**/*.less\"",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp"
